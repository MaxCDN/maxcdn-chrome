{
  "name": "MaxCDN Chrome",
  "version": "0.0.4",
  "manifest_version": 3,
  "description": "View your MaxCDN Chrome Extension.",
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://; object-src 'self'",
  "homepage_url": "http://www.maxcdn.com/company/open-source/",
  "icons": {
    "16": "assets/img/icon16.png",
    "48": "assets/img/icon48.png",
    "128": "assets/img/icon128.png"
  },
  "browser_action": {
    "default_icon": "assets/img/icon19.png",
    "default_title": "MaxCDN Extension",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "https://rws.maxcdn.com/*",
    "tabs",
    "storage"
  ]
}
